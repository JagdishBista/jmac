<odoo>
    <data noupdate="1">
        <record id="payment_token_public_user_rule" model="ir.rule">
            <field name="name">Access own payment tokens only (for public user)</field>
            <field name="model_id" ref="payment.model_payment_token"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('base.group_public'))]"/>
        </record>
    </data>
</odoo>
