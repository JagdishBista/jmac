<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="filter_sale_order" model="ir.filters">
            <field name="name">Unprocessed Orders</field>
            <field name="model_id">sale.order</field>
            <field name="is_default">True</field>
            <field name="domain">[("is_unprocessed_order","=","True")]</field>
            <field name="user_id" eval="False"/>
            <field name="action_id" ref="sale.action_orders"/>
            <field name="context">{}</field>
        </record>

        <record id="filter_stock_picking" model="ir.filters">
            <field name="name">Ship Picking</field>
            <field name="model_id">stock.picking</field>
            <field name="is_default">True</field>
            <field name="domain">[("state","=","assigned"),("carrier_id","!=",False)]</field>
            <field name="user_id" eval="False"/>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>
